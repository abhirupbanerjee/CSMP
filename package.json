{
  "name": "csmp-bot-vercel",
  "version": "1.0.0",
  "description": "Caribbean Government Service Portal - Multi-Agent Web Platform",
  "main": "src/index.js",
  "homepage": ".",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:frontend\"",
    "dev:api": "node server.js",
    "dev:frontend": "react-scripts start",
    "dev:api-only": "node server.js",
    "dev:frontend-only": "react-scripts start",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "vercel-build": "react-scripts build",
    "validate-config": "node -e \"require('./shared/service-loader.js').validateRepository()\"",
    "serve": "serve -s build",
    "test:api": "node -e \"require('./test-api.js')\"",
    "setup": "npm install && echo 'Setup complete! Run: npm run dev'"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "serve": "^14.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "caribbean",
    "government",
    "chatbot",
    "multi-agent",
    "trinidad-tobago",
    "openai",
    "react",
    "government-services"
  ],
  "author": {
    "name": "Caribbean Government Service Portal",
    "email": "info@caribbeanportal.gov.tt",
    "url": "https://github.com/your-org/csmp-bot-vercel"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/csmp-bot-vercel.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/csmp-bot-vercel/issues"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  }
}
